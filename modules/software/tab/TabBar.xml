<f:TabBar class="TabBar" more="true" selectTime="touch_begin" percentWidth="100" height="25"
          creationComplete="this.init();" xmlns:f="flower"
          xmlns:s="./../module.json">
    <f:script src="./TabBar.script"/>
    <f:itemRenderer>
        <f:ItemRenderer percentHeight="100">
            <f:Rect percentWidth="100" percentHeight="100" fillColor.up="0xcccccc" fillColor.down="0xcccccc"
                      fillColor.selectedDown="0xffffff" fillColor.selectedUp="0xffffff"
                      lineColor="0x333333"
                      lineWidth="1"/>
            <f:Image source="{data.icon}"/>
            <f:Label x="30" text="{data.title}        " verticalCenter="0"/>
        </f:ItemRenderer>
    </f:itemRenderer>
    <f:moreButton>
        <f:Button percentHeight="100">
            <f:Rect percentWidth="100" percentHeight="100" fillColor.up="0xcccccc" fillColor.down="0xffffff"
                      lineColor="0x333333"
                      lineWidth="1"/>
            <f:Label text="..." horizontalCenter="0" verticalCenter="0"/>
        </f:Button>
    </f:moreButton>
</f:TabBar>