<f:List class="LabelList" selectTime="touch_end" xmlns:f="flower">
    <f:itemRenderer>
        <f:ItemRenderer name="test" class="LabelItemRenderer" percentWidth="100" height="20">
            <f:Rect percentWidth="100" percentHeight="100" fillColor.down="0xd6d4d0"
                    fillColor.selectedDown="0x96b97d" fillColor.selectedUp="0x96b97d"
                    fillColor.over="0x98b7dd"
                    alpha.up="0" alpha.over="0.5"
                    alpha.down="1" alpha.selectedDown="1"
                    alpha.selectedUp="1"/>
            <f:Label text="{data.label}" horizontalCenter="0" verticalCenter="0" fontColor="0x000000"/>
        </f:ItemRenderer>
    </f:itemRenderer>
</f:List>