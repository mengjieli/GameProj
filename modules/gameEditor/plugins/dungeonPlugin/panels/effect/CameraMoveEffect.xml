<f:Group class="CameraMoveEffect" percentWidth="100" percentHeight="100"
         xmlns:f="flower" xmlns:s="modules/software/module.json"
         xmlns:e="modules/gameEditor/module.json" >
    <f:script src="./CameraMoveEffect.script"/>
    <f:Group horizontalCenter="0">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标X" verticalCenter="0"/>
        <s:Input id="coordxInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="30">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标Y" verticalCenter="0"/>
        <s:Input id="coordyInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="60">
        <f:Label horizontalCenter="-55" width="60" text="时间(ms)" verticalCenter="0"/>
        <s:Input id="timeInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="90">
        <f:Label horizontalCenter="-55" width="60" text="坐标位置" verticalCenter="0"/>
        <s:ComboBox id="posComboBox" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
</f:Group>