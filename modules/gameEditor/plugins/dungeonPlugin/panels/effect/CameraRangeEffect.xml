<f:Group class="CameraRangeEffect" percentWidth="100" percentHeight="100"
         xmlns:f="flower" xmlns:s="modules/software/module.json"
         xmlns:e="modules/gameEditor/module.json" >
    <f:script src="./CameraRangeEffect.script"/>
    <f:Group horizontalCenter="0">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标X" verticalCenter="0"/>
        <s:Input id="rectxInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="30">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标Y" verticalCenter="0"/>
        <s:Input id="rectyInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="60">
        <f:Label horizontalCenter="-55" width="60" text="像素宽" verticalCenter="0"/>
        <s:Input id="rectwidthInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="90">
        <f:Label horizontalCenter="-55" width="60" text="像素高" verticalCenter="0"/>
        <s:Input id="rectheightInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
</f:Group>