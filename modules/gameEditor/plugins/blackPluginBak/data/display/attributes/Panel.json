  "members": {
    "title": {
      "type": "uieditor.AttributeString"
    },
    "icon": {
      "type": "uieditor.AttributeString"
    },
    "scaleMode": {
      "type": "uieditor.AttributeString",
      "init": {
        "content": "no_scale",
        "list": [
          {
            "label": "NoScale",
            "type": "no_scale"
          },
          {
            "label": "ShowAll",
            "type": "show_all"
          },
          {
            "label": "NoBorder",
            "type": "no_border"
          },
          {
            "label": "ScaleWidth",
            "type": "scale_width"
          },
          {
            "label": "ScaleHeight",
            "type": "scale_height"
          }
        ]
      }
    }
  }
}